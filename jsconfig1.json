{
  "endpoints": {
    "randomNumber": {
      "url": "http://www.randomnumberapi.com/api/v1.0/random",
      "description": "Get a random number",
      "options": {
        "min": {
          "description": "The minimum random number",
          "default": 0
        },
        "max": {
          "description": "The maximum random number",
          "default": 100
        },
        "count": {
          "description": "The number of random numbers to generate (Maximum of 100)",
          "default": 1
        }
      },
      "example": "http://www.randomnumberapi.com/api/v1.0/random?min=100&max=1000&count=5"
    },
    "randomRedditNumber": {
      "url": "http://www.randomnumberapi.com/api/v1.0/randomredditnumber",
      "description": "Get a random number using the reddit comment feed as the seed",
      "options": {
        "min": {
          "description": "The minimum random number",
          "default": 0
        },
        "max": {
          "description": "The maximum random number",
          "default": 100
        },
        "count": {
          "description": "The number of random numbers to generate (Maximum of 100)",
          "default": 1
        }
      },
      "example": "http://www.randomnumberapi.com/api/v1.0/randomredditnumber?min=100&max=1000&count=5"
    },
    "randomString": {
      "url": "http://www.randomnumberapi.com/api/v1.0/randomstring",
      "description": "Get a random string",
      "options": {
        "min": {
          "description": "The minimum number of characters in the string (Max 1000)",
          "default": 10
        },
        "max": {
          "description": "The maximum number of characters in the string (Max 1000)",
          "default": 20
        },
        "count": {
          "description": "The number of random strings to generate (Maximum of 100)",
          "default": 1
        },
        "all": {
          "description": "Include numbers and symbols (True/False)",
          "default": false
        }
      },
      "example": "http://www.randomnumberapi.com/api/v1.0/randomstring?min=5&max=20&count=5"
    },
    "randomUUID": {
      "url": "http://www.randomnumberapi.com/api/v1.0/uuid",
      "description": "Get a random UUID",
      "options": {
        "count": {
          "description": "The number of random numbers to generate (Maximum of 100)",
          "default": 1
        }
      },
      "example": "http://www.randomnumberapi.com/api/v1.0/uuid?count=2"
    }
  }
}
